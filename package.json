{
  "name": "neptunium-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run build:check-rust && next build",
    "build:check-rust": "if [ \"$NETLIFY\" = \"true\" ]; then npm run build:rust-netlify; else npm run build:rust; fi",
    "build:rust": "cd rust-backend && chmod +x build.sh && ./build.sh",
    "build:rust-netlify": "echo '⚠️ Skipping Rust build on Netlify - using pre-built WASM' && npm run build:rust-fallback",
    "build:rust-fallback": "mkdir -p src/lib/wasm && echo 'export default function init() { return Promise.resolve(); }' > src/lib/wasm/neptunium_core.js && echo 'export {}' > src/lib/wasm/neptunium_core.d.ts",
    "start": "next start",
    "lint": "next lint",
    "clean": "rm -rf .next rust-backend/target src/lib/wasm",
    "test:rust": "cd rust-backend && cargo test"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.864.0",
    "@aws-sdk/s3-request-presigner": "^3.864.0",
    "@headlessui/react": "^2.2.7",
    "@heroicons/react": "^2.2.0",
    "@react-email/components": "^0.5.0",
    "@react-email/render": "^1.2.0",
    "@supabase/supabase-js": "^2.55.0",
    "@upstash/redis": "^1.35.3",
    "apexcharts": "^4.0.0",
    "autoprefixer": "^10.4.21",
    "bcryptjs": "^2.4.3",
    "clsx": "^2.1.1",
    "flatpickr": "^4.6.13",
    "jsonwebtoken": "^9.0.2",
    "jsvectormap": "^1.6.0",
    "lucide-react": "^0.539.0",
    "next": "15.4.6",
    "next-auth": "^4.24.11",
    "postcss": "^8.5.6",
    "react": "19.1.0",
    "react-apexcharts": "^1.7.0",
    "react-dom": "19.1.0",
    "react-email": "^4.2.8",
    "react-hot-toast": "^2.4.1",
    "resend": "^6.0.1",
    "tailwindcss": "^4.0.0",
    "@tailwindcss/postcss": "^4.0.0",
    "typescript": "^5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@netlify/functions": "^4.2.1",
    "eslint": "^9",
    "eslint-config-next": "15.4.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20"
  }
}